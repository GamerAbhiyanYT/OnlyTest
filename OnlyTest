local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/Kronos"))()

local window = library:Window({
   Title = "ExplanHUB - By GamerAbhiyan",
   Accent = Color3.fromRGB(69,69,207),
   Logo = 13853292478,
   ToggleKey = Enum.KeyCode.Q
})

local tab = window:NewTab({
   Logo = 4483345998
})

local tabsection = tab:TabSection({
   Title = "Select Menu"
})

local column = tabsection:AddColumn({
   Title = "Key Toggle"
})

local section = column:Section({
   Title = "Key Toggle"
})

section:Button({
   Text = "Keyboard",
   Callback = function(loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()) end
})

section:Keybind({
   Text = "Keybind",
   Key = Enum.KeyCode.Q,
   Callback = function() end
})

local column = tabsection:AddColumn({
   Title = "Recommended"
})

section:TextLabel({
   Text = "I Still Recommend To Always Press This After Executing This Script"
})

section:Button({
   Text = "Rejoin Button On Disconnetced",
   Callback = function(loadstring(game:HttpGet("https://raw.githubusercontent.com/rouxhaver/scripts-2/main/rejoin%20button%20for%20kick%20message.lua"))()) end
})

section:TextLabel({
   Text = "If You Die And Lose Toggle"
})

section:TextLabel({
   Text = "Press This Button To Bring Up Toggle Back"
})

section:Button({
   Text = "Bring Up The Toggles",
   Callback = function(loadstring(game:HttpGet("https://raw.githubusercontent.com/GamerAbhiyanYT/Explan/main/Toggle"))()) end
})

local column = tabsection:AddColumn({
   Title = "Home"
})

local section = column:Section({
   Title = "WalkSpeed And JumpPower"
})

section:Slider({
   Text = "WalkSpeed",
   Min = 16,
   Max = 100,
   Def = 16,
   Callback = function(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed) end
})

section:Slider({
   Text = "JumpPower",
   Min = 50,
   Max = 100,
   Def = 50,
   Callback = function(game.Players.LocalPlayer.Character.Humanoid.JumpPower) end
})

section:Textbox({
   Text = "WalkSpeed",
   Callback = function(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed) end
})

section:Textbox({
   Text = "JumpPower",
   Callback = function(game.Players.LocalPlayer.Character.Humanoid.JumpPower) end
})
